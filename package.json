{
  "name": "json-schema-typescript-generator",
  "version": "2.2.4",
  "author": {
    "name": "Nathan Adams",
    "url": "https://github.com/NathanJAdams"
  },
  "license": "MIT",
  "homepage": "https://github.com/NathanJAdams/json-schema-to-ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/NathanJAdams/json-schema-to-ts"
  },
  "keywords": [
    "generate",
    "file",
    "json",
    "schema",
    "json-schema",
    "ts",
    "typescript"
  ],
  "files": [
    "dist/",
    "LICENSE",
    "README.md"
  ],
  "bin": {
    "json-schema-typescript-generator": "./dist/cjs/cli/generate.js"
  },
  "main": "./dist/cjs/index.cjs.js",
  "module": "./dist/esm/index.esm.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.esm.js",
      "require": "./dist/cjs/index.cjs.js",
      "types": "./dist/types/index.d.ts",
      "default": "./dist/esm/index.esm.js"
    }
  },
  "scripts": {
    "build": "yarn clean && yarn build:cjs && yarn build:esm && yarn build:types",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build:types": "tsc --project tsconfig.types.json",
    "clean": "rimraf ./dist/",
    "lint": "eslint . --fix",
    "test": "jest --silent=false"
  },
  "dependencies": {
    "commander": "^14.0.0",
    "rimraf": "^6.0.1"
  },
  "devDependencies": {
    "@types/jest": "30.0.0",
    "@types/node": "^20",
    "@typescript-eslint/eslint-plugin": "8.38.0",
    "@typescript-eslint/parser": "8.38.0",
    "eslint": "9.32.0",
    "jest": "30.0.5",
    "ts-jest": "29.4.0",
    "typescript": "5.8.3"
  }
}